<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:startColor="#5bc0de" android:endColor="#339bb9" />
			<stroke android:width="1dp" android:color="#339BB9" />
			<corners android:radius="3dp" />
			<padding android:left="10dp" android:top="10dp"
				android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient android:startColor="#ffffff" android:endColor="#e6e6e6"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#C0C0C0" />
			<corners android:radius="4dp" />
			<padding android:left="10dp" android:top="10dp"
				android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
</selector>
